<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="horizontal" >
    <TextView
        android:id="@+id/textStockName"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:textColor="#ffffff"
        android:textSize="30px" />
    <TextView
        android:id="@+id/textStockNum"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:textColor="#ffffff"
        android:textSize="30px" />
    <TextView
        android:id="@+id/textPrice"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:textColor="#ffffff"
        android:textSize="30px" />
    <TextView
        android:id="@+id/textNormalByPer"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:textColor="#ffffff"
        android:textSize="30px" />
    <TextView
        android:id="@+id/textNormalByPbr"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:textColor="#ffffff"
        android:textSize="30px" />
    <TextView
        android:id="@+id/textGvi"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:textColor="#ffffff"
        android:textSize="30px" />
    <TextView
        android:id="@+id/textPsr"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:textColor="#ffffff"
        android:textSize="30px" />
    <TextView
        android:id="@+id/textRoe"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:textColor="#ffffff"
        android:textSize="30px" />
    <TextView
        android:id="@+id/textCapital"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:textColor="#ffffff"
        android:textSize="30px" />
    <TextView
        android:id="@+id/textBookValue"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:textColor="#ffffff"
        android:textSize="30px" />
    <TextView
        android:id="@+id/textIndustry"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:textColor="#ffffff"
        android:textSize="30px" />	                					
</LinearLayout>